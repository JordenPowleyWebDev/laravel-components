{
    "name": "jordenpowleywebdev/laravel-components",
    "description": "This is my package laravel-components",
    "keywords": [
        "JordenPowleyWebDev",
        "laravel",
        "laravel-components"
    ],
    "homepage": "https://github.com/jordenpowleywebdev/laravel-components",
    "license": "MIT",
    "authors": [
        {
            "name": "JordenPowleyWebDev",
            "email": "jorden.powley.webdev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "JordenPowleyWebDev\\LaravelComponents\\": "src",
            "JordenPowleyWebDev\\LaravelComponents\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JordenPowleyWebDev\\LaravelComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JordenPowleyWebDev\\LaravelComponents\\LaravelComponentsServiceProvider"
            ],
            "aliases": {
                "LaravelComponents": "JordenPowleyWebDev\\LaravelComponents\\Facades\\LaravelComponents"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}